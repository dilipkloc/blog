<h1> articles index </h1>
<% @articles.each do |article|%>
 <b> Category: <%= Category.find(article.category_id).name %> </b> <br/>
  <%= article.title %><br/>
  <%= article.body %><br/>
  <%= article.category_id %><br/>
  <%= article.publish_date %><br/>
  <% if article.is_published %>
    It is published
  <% else %>
    It is not published
  <% end %>
  <br/>
  <img src=<%= article.feature_image_url %> height="42" width="42">
   <br/><br/>
  
<% end %>